
<article class="sticker-pack">
  <% data.stickers.stickers.each do |f| %>
  <img
    src="<%= image_path f.path, resize: "95x95" %>"
    width="<%= f.originalWidth %>"
    height="<%= f.originalHeight %>"
    alt=""
    class="sticker <%= f.className %>"
    sizes=" (min-width: 1024px) 210px,  95px"
    srcset="
    <%= image_path f.path, resize: "95x95" %> 95w,
    <%= image_path f.path, resize: "190x190" %> 190w,
    <%= image_path f.path, resize: "210x210" %> 210w,
    <%= image_path f.path, resize: "285x285" %> 285w,
    <%= image_path f.path, resize: "420x420" %> 420w,
    <%= image_path f.path, resize: "630x630" %> 630w
    "
  >
  <% end %>

</article>
